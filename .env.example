# Environment variables
OPENAI_API_KEY=your-openai-api-key-here
QDRANT_API_KEY=

# Vector Store Configuration
OPENRAG_VECTOR_STORE__TYPE=qdrant
OPENRAG_VECTOR_STORE__QDRANT__HOST=localhost
OPENRAG_VECTOR_STORE__QDRANT__PORT=6333

# LLM Configuration
OPENRAG_LLM__PROVIDER=openai
OPENRAG_LLM__OPENAI__MODEL=gpt-3.5-turbo
OPENRAG_LLM__OPENAI__TEMPERATURE=0.7

# API Configuration
OPENRAG_API__HOST=0.0.0.0
OPENRAG_API__PORT=8000

# Logging
OPENRAG_LOGGING__LEVEL=INFO
